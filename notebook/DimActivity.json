						}
					}
				},
				"source": [
					"logDF = logDF.groupBy('Event name').count().withColumn('ActivitySK', row_number().over(w))\r\n",
					"logDF = logDF.withColumnRenamed('count', 'NumberOfActivities')\r\n",
					"logDF = logDF.withColumnRenamed('Event name', 'ActivityName')"
				],
